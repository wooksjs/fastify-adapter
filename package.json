{
  "name": "@wooksjs/fastify-adapter",
  "version": "0.2.0",
  "description": "Fastify Adapter for Wooks Composables",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "release": "node ./scripts/release",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage",
    "lint": "eslint --ext .ts src/**/**.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooksjs/fastify-adapter.git"
  },
  "exports": {
    ".": {
      "node": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      }
    }
  },
  "keywords": [
    "http",
    "wooks",
    "composables",
    "fastify",
    "adapter",
    "web",
    "framework",
    "app",
    "api",
    "rest",
    "restful",
    "prostojs"
  ],
  "buildOptions": {
    "formats": [
      "esm-bundler",
      "cjs"
    ]
  },
  "gitHooks": {
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "author": "Artem Maltsev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wooksjs/fastify-adapter/issues"
  },
  "homepage": "https://github.com/wooksjs/fastify-adapter#readme",
  "peerDependencies": {
    "@wooksjs/event-http": "^0.3.3",
    "fastify": "^4.0.0"
  },
  "devDependencies": {
    "@prostojs/dye": "^0.3.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@wooksjs/event-http": "^0.3.3",
    "@wooksjs/http-body": "^0.3.3",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "execa": "^5.1.1",
    "fastify": "^4.9.2",
    "jest": "^29.2.2",
    "minimist": "^1.2.6",
    "semver": "^7.3.5",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4",
    "yorkie": "^2.0.0"
  }
}
